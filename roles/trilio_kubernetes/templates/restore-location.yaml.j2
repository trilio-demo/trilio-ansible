apiVersion: triliovault.trilio.io/v1
kind: Restore
metadata:
  name: {{ trilio_kubernetes_backup_prefix }}-migration-restore
spec:
  source:
    type: Location
    location: {% for item in location %}{{ item }}{% endfor %} 
    target:
      name: {{ trilio_kubernetes_target_name }}
      namespace: {{ trilio_kubernetes_migrate_namespace }}
  skipIfAlreadyExists: true
